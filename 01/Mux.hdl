/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=nsel);
    Nand(a=nsel, b=a, out=outa);
    Nand(a=sel, b=b, out=outb);
    Nand(a=outa, b=outb, out=out);
    
}